<HTML>
<HEAD>
<SCRIPT LANGUAGE="JavaScript">
function dil(form)
{
   for(var i=0; i<form.elements.length; i++)
   {
		if(form.elements[i].value == "")
		{
		   alert("Fill out all Fields")
		   document.F1.acno.focus()
		   return false
		}
   }

   if(isNaN(document.F1.acno.value))
   {
       alert("A/C No.  must  be  number & can't be null")
	   document.F1.acno.value=""
	   document.F1.acno.focus()
	   return false
   }

   if(!isNaN(document.F1.uname.value))
   {
       alert("User Name  must  be  char's & can't be null")
	   document.F1.uname.value=""
	   document.F1.uname.focus()
	   return false
   }

   if(!isNaN(document.F1.pass.value))
   {
       alert("Password  must  be  char's & can't be null")
	   document.F1.pass.value=""
	   document.F1.pass.focus()
	   return false
   }

   return true   
}
</SCRIPT>
</HEAD>

<BODY BGCOLOR="#d0d0d0">
<FORM NAME="F1" METHOD=POST onSubmit="return dil(this)" ACTION="./closecli">
<CENTER><FONT SIZE="5" COLOR="red">WELCOME&nbsp; TO&nbsp; ACCOUNT&nbsp; CLOSING&nbsp; FORM</FONT><BR><BR><BR><BR><BR><br><br>
ACCOUNT &nbsp; NUMBER&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=TEXT  name="acno"SIZE=22><BR><BR><td align=left>
USER&nbsp;NAME&nbsp;&nbsp; &nbsp; &nbsp;  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=TEXT name="uname" SIZE=22><BR><BR>
PASSWORD&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=password  name="pass" SIZE=22><BR><BR><BR><BR>
<INPUT TYPE=SUBMIT VALUE="SUBMIT">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
<INPUT TYPE=RESET VALUE="CLEAR"><BR><BR><BR>
<A href="Main.html"><IMG SRC="home1.gif"></IMG></A>
</CENTER>
</FORM>
</BODY>
</HTML>