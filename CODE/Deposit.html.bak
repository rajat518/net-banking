<HTML>
<HEAD>
<SCRIPT LANGUAGE="JavaScript">
function dil(form)
{
   for(var i=0; i<form.elements.length; i++)
   {
		if(form.elements[i].value == "")
		{
		   alert("Fill out all Fields")
		   document.F1.acno.focus()
		   return false
		}
   }

   if(isNaN(document.F1.acno.value))
   {
       alert("A/C No.  must  be  number & can't be null")
	   document.F1.acno.value=""
	   document.F1.acno.focus()
	   return false
   }

   if(!isNaN(document.F1.uname.value))
   {
       alert("User Name  must  be  char's & can't be null")
	   document.F1.uname.value=""
	   document.F1.uname.focus()
	   return false
   }

   if(!isNaN(document.F1.pwd.value))
   {
       alert("Password  must  be  char's & can't be null")
	   document.F1.pwd.value=""
	   document.F1.pwd.focus()
	   return false
   }

   if(isNaN(document.F1.amt.value))
   {
       alert("Amount  must  be  number & can't be null")
	   document.F1.amt.value=""
	   document.F1.amt.focus()
	   return false
   }

   return true   
}
</SCRIPT>

</HEAD>

<BODY BGCOLOR="#d0d0d0">
<FORM NAME=F1 METHOD="POST" onSubmit="return dil(this)" ACTION="./depocli">
<CENTER><FONT SIZE="5" COLOR="RED">WELCOME&nbsp; TO&nbsp; DEPOSIT&nbsp; FORM</FONT><BR><BR><BR><BR>
ACCOUNT&nbsp; NO&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=TEXT SIZE=22 NAME="acno"><BR><BR>
USER&nbsp; NAME&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=TEXT SIZE=22 name="uname"><BR><BR>
PASSWORD&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=password SIZE=22 name="pwd"><BR><BR>
AMOUNT &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<INPUT TYPE=TEXT SIZE=22 name="amt"><BR><BR><BR><BR>
<INPUT TYPE=SUBMIT VALUE="DEPOSIT">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
<INPUT TYPE=RESET VALUE="CLEAR"><BR><BR><BR>
<A href="Main.html"><IMG SRC="home1.gif"></IMG></A>
</CENTER>
</FORM>
</BODY>
</HTML>